{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Docker Compose Up",
            "type": "docker-compose",
            "dockerCompose": {
                "up": {
                    "detached": true,
                    "build": true,
                },
                "files": [
                    "${workspaceFolder}/docker-compose.yml"
                ]
            },
            "dependsOn": [
                "Docker Compose Down"
            ]
        },
        {
            "label": "Docker Compose Down",
            "type": "docker-compose",
            "dockerCompose": {
                "down": {
                },
                "files": [
                    "${workspaceFolder}/docker-compose.yml"
                ]
            }
        }
    ]
}